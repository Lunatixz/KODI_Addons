msgctxt "Addon Summary"
msgid "Integrate IPTV channels from other add-ons in the Kodi TV and Radio menus"
msgstr ""

msgctxt "Addon Description"
msgid "IPTV Manager integrates IPTV channels from other add-ons in the Kodi TV and Radio menus."
msgstr ""

# CONTEXT MENU
msgctxt "#30600"
msgid "Play with IPTV Manager"
msgstr "Odtwarzaj z IPTV Manager"

# MESSAGES
msgctxt "#30700"
msgid "Are you sure you want to setup IPTV Simple for the use with IPTV Manager? Any existing configuration of IPTV Simple will be overwritten."
msgstr "Czy na pewno chcesz skonfigurować IPTV Simple z IPTV Manager? Każda istniejąca konfiguracja IPTV Simple zostanie nadpisana."

msgctxt "#30701"
msgid "IPTV Simple is configured for use with IPTV Manager!"
msgstr "IPTV Simple jest skonfigurowany do użytku z IPTV Manager!"

msgctxt "#30702"
msgid "The configuration of IPTV Simple has failed. Please check the log files for more information."
msgstr "Konfiguracja IPTV Simple nie powiodła się. Sprawdź pliki rejestru, aby uzyskać więcej informacji."

msgctxt "#30703"
msgid "Detecting IPTV add-ons..."
msgstr "Wykrywanie dodatków IPTV..."

msgctxt "#30704"
msgid "Fetching channels and guide of {addon}..."
msgstr "Pobieranie kanałów i przewodnika {addon}..."

msgctxt "#30705"
msgid "Updating channels and guide..."
msgstr "Aktualizowanie kanałów i przewodnika..."

msgctxt "#30706"
msgid "This program isn't available to play."
msgstr "Ten program nie jest dostępny do odtwarzania."

# SETTINGS
msgctxt "#30800"
msgid "Channels"
msgstr "Kanały"

msgctxt "#30801"
msgid "Refreshing"
msgstr "Odświeżanie"

msgctxt "#30802"
msgid "Refresh interval [I](in hours)[/I]"
msgstr "Interwał odświeżania [I](w godzinach)[/I]"

msgctxt "#30803"
msgid "Refresh channels and guide now…"
msgstr "Odśwież kanały i przewodnika teraz…"

msgctxt "#30820"
msgid "IPTV Simple"
msgstr "IPTV Simple"

msgctxt "#30821"
msgid "Configuration"
msgstr "Konfiguracja"

msgctxt "#30822"
msgid "Configure IPTV Simple automatically…"
msgstr "Skonfiguruj IPTV Simple automatycznie…"

msgctxt "#30823"
msgid "Open IPTV Simple settings…"
msgstr "Otwórz ustawienia IPTV Simple…"

msgctxt "#30824"
msgid "Automatically restart IPTV Simple to refresh the data"
msgstr "Automatycznie uruchom ponownie IPTV Simple, aby odświeżyć dane"

msgctxt "#30880"
msgid "Expert"
msgstr "Eksperckie"

msgctxt "#30881"
msgid "Logging"
msgstr "Rejestrowanie"

msgctxt "#30882"
msgid "Enable debug logging"
msgstr "Włącz rejestrowanie debugowania"

msgctxt "#30883"
msgid "Install Kodi Logfile Uploader…"
msgstr "Zainstaluj Kodi Logfile Uploader…"

msgctxt "#30884"
msgid "Open Kodi Logfile Uploader…"
msgstr "Otwórz Kodi Logfile Uploader…"
